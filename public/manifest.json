  "screenshots": [
    {
      "src": "/screenshots/desktop-home.png",
      "sizes": "1280x720",
      "type": "image/png",
      "form_factor": "wide",
      "label": "RR Equipment Homepage"
    },
    {
      "src": "/screenshots/mobile-shop.png",
      "sizes": "390x844",
      "type": "image/png",
      "form_factor": "narrow",
      "label": "Equipment Shop"
    },
    {
      "src": "/screenshots/desktop-product.png",
      "sizes": "1280x720",
      "type": "image/png",
      "form_factor": "wide",
      "label": "Product Details"
    },
    {
      "src": "/screenshots/mobile-blog.png",
      "sizes": "390x844",
      "type": "image/png",
      "form_factor": "narrow",
      "label": "Industry Blog"
    }
  ],
  "shortcuts": [
    {
      "name": "Shop Equipment",
      "short_name": "Shop",
      "description": "Browse construction equipment",
      "url": "/shop",
      "icons": [
        {
          "src": "/icons/shortcut-shop.png",
          "sizes": "96x96",
          "type": "image/png"
        }
      ]
    },
    {
      "name": "Industry Blog",
      "short_name": "Blog",
      "description": "Read industry insights",
      "url": "/blog",
      "icons": [
        {
          "src": "/icons/shortcut-blog.png",
          "sizes": "96x96",
          "type": "image/png"
        }
      ]
    },
    {
      "name": "Sign In",
      "short_name": "Login",
      "description": "Access your account",
      "url": "/auth/signin",
      "icons": [
        {
          "src": "/icons/shortcut-login.png",
          "sizes": "96x96",
          "type": "image/png"
        }
      ]
    }
  ],
  "related_applications": [
    {
      "platform": "webapp",
      "url": "https://rrequipment.com/manifest.json"
    }
  ],
  "prefer_related_applications": false,
  "edge_side_panel": {
    "preferred_width": 480
  },
  "launch_handler": {
    "client_mode": "navigate-existing"
  },
  "protocol_handlers": [
    {
      "protocol": "mailto",
      "url": "/contact?email=%s"
    }
  ],
  "file_handlers": [
    {
      "action": "/upload",
      "accept": {
        "image/*": [".jpg", ".jpeg", ".png", ".gif", ".webp"],
        "application/pdf": [".pdf"]
      }
    }
  ],
  "share_target": {
    "action": "/share",
    "method": "POST",
    "enctype": "multipart/form-data",
    "params": {
      "title": "title",
      "text": "text",
      "url": "url",
      "files": [
        {
          "name": "images",
          "accept": ["image/*"]
        }
      ]
    }
  }
} 